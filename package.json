{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite --host --port 5173",
    "dev:backend": "tsx watch src/backend/demoServer.ts",
    "dev:full": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:auto:full": "concurrently --kill-others-on-fail --prefix-colors \"cyan,green,yellow,blue\" --names \"FRONTEND,BACKEND,TYPESCRIPT,LINT\" \"npm run dev:frontend\" \"npm run dev:backend\" \"tsc --project tsconfig.app.json --watch --noEmit\" \"npm run lint:watch\"",
    "build": "vite build",
    "build:backend": "tsc --project tsconfig.backend.json --watch",
    "lint": "eslint .",
    "lint:watch": "eslint . --watch --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --project tsconfig.app.json --noEmit",
    "type-check:watch": "tsc --project tsconfig.app.json --watch --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@supabase/supabase-js": "^2.55.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chokidar": "^4.0.3",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
